{
  "name": "cinecorner-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "tsc --noEmit && eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "npm run test:unit -- --collectCoverage && npm run test:feature -- --collectCoverage && npm run coverage:report",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:feature": "jest --config ./jest-feature.ts",
    "prepare": "husky",
    "pre-push": "npm run lint && npm run test",
    "coverage:report": "rm -f coverage/*.json && cp coverage/unit/coverage-final.json coverage/unit.coverage-final.json && cp coverage/feature/coverage-final.json coverage/feature.coverage-final.json && nyc merge coverage/ coverage/coverage-final.json && nyc report --temp-dir ./coverage --check-coverage --reporter=lcov --reporter=text-summary",
    "coverage:html": "nyc report --temp-dir ./coverage --reporter=html && open coverage/index.html"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "qs": "^6.13.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/qs": "^6.9.16",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fetch-mock": "^12.1.0",
    "husky": "^9.1.6",
    "jest": "^29.5.0",
    "nyc": "^17.1.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
